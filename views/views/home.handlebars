<div class ="page-header">
	<h3>Welcome to MuseList</h3>
</div>

<div class="page-header">
<h3>Search Feature</h3>
</div>
<div class="searchbar">
<h4>Search for an Artist, see 5 of their songs</h4>
<form class="formSearchbar" role="form"
  action="/process?form=searchbar" method="POST">

  <div class="form-group">
    <label for="fieldArtist" class="col-sm-2 control-label">
    Artist</label>
    <div class="col-sm-4">
      <input type="text" class="form-control" id="fieldArtist"
      name="artist">
    </div>
  </div> <!-- End of form-group -->

  <div class="form-group">
    <div class="col-sm-4">
      <button type="submit" class="btn btn-default">Submit</button>
    </div>
  </div> <!-- End of form-group -->

</form>
</div> <!-- End of searchbar -->

<div class="buttons">
<form class="formDB" role="form"
  action="/list" method="GET">

  <div class="form-group">
    <div class="col-sm-4">
      <button type="submit" class="btn btn-default">View DB</button>
    </div>
  </div>
</form>
<form class="addToDB" role="form"
  action="/insert" method="GET">

  <div class="addToDB">
    <div class="col-sm-4">
      <button type="submit" class="btn btn-default">Insert to DB</button>
    </div>
  </div>
</form>

</div> <!-- end of buttons -->

